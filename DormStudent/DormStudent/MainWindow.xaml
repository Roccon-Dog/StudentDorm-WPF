<Window x:Class="DormStudent.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DormStudent"
        mc:Ignorable="d"
        Title="选择登录注册界面" Height="249.6" Width="324.8">
    <Grid Margin="0,0,3.6,0.4">
        <Grid.RowDefinitions>
            <RowDefinition Height="94*"/>
            <RowDefinition Height="125*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="119*"/>
            <ColumnDefinition Width="675*"/>
        </Grid.ColumnDefinitions>
        <Button Content="登录" HorizontalAlignment="Left" Margin="60.8,18.6,0,0" VerticalAlignment="Top" Width="98" Height="33" Grid.Column="1" Grid.Row="1" FontSize="22" Click="onLogin"/>
        <Button Content="注册" HorizontalAlignment="Left" Margin="60.8,68.6,0,0" VerticalAlignment="Top" Width="98" Height="33" Grid.Column="1" Grid.Row="1" FontSize="22" Click="onSignin"/>
        <Label Grid.ColumnSpan="2" Content="宿舍管理系统" HorizontalAlignment="Left" Height="64" Margin="0,10,-0.6,0" VerticalAlignment="Top" Width="315" HorizontalContentAlignment="Center" FontSize="24" VerticalContentAlignment="Center"/>


    </Grid>
</Window>
